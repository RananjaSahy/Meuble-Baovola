    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreListeStyle</servlet-name>
        <url-pattern>/PreListeStyle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreListeMatiereStyle</servlet-name>
        <url-pattern>/PreListeMatiereStyle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertionVolume</servlet-name>
        <url-pattern>/InsertionVolume</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
