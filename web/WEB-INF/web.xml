        <servlet-name>PreInsertionStyle</servlet-name>
        <servlet-class>controller.insertion.PreInsertionStyle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreListeStyle</servlet-name>
        <servlet-class>controller.liste.PreListeStyle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreListeMatiereStyle</servlet-name>
        <servlet-class>controller.liste.PreListeMatiereStyle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertionVolume</servlet-name>
        <servlet-class>controller.insertion.InsertionVolume</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertionMatiere</servlet-name>
        <url-pattern>/InsertionMatiere</url-pattern>
    </servlet-mapping>
